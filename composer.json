            "Izzle\\TokenHandler\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/encryption": "^8.22.1",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
