{
    "name": "izzle/token-handler",
    "description": "Simple token handler lib",
    "keywords": ["model", "lib"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel BÃ¶hmer",
            "email": "daniel@izzle.org"
        }
